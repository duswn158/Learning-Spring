<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="oh" class="com.test02.Emp">
		<property name="name" value="오승민"/>
		<property name="salary" value="500"/>
	</bean>
	
	<bean id="ingu" class="com.test02.Emp">
		<constructor-arg index="0" value="황인규"></constructor-arg>
		<constructor-arg name="salary" value="4000"></constructor-arg>
	</bean>

</beans>
